<script src="https://upload-widget.cloudinary.com/global/all.js" type="text/javascript"></script>  
<script type="text/javascript"></script>
<div id="app"></div>
<div>
    <button id="upload_widget"
    class="cloudinary_button">
        Add Profile Image here!
    </button>
    <br>
<img id="uploadedimage" 
      src="">
    </img> 
</div>
  <br>
  <h3>Nurse ID:{{nurse_id}}</h3>
  <h3>Name:</h3>
  {{name}}
  <br>
  <br>
  <h3>Email:</h3>
  {{email}}
  <br>
  <br>
  <h3>Bio:</h3>
  {{bio}}
  <br>
  <br>
  <h3>Shift Info:</h3>
  {{ shift_info }}
  <br>
  <br>
  <h3>Location:</h3>
  <p>{{street_address}}, {{city}}, {{zip_code}}</p>
  <br>
  <br>
  <h3>Drives:</h3>
  {{is_driving}}
  <br>
  <br>
  <h3>Car Info:</h3>
  <p>{{car_make}}, {{car_model}}, {{car_color}}</p>
  <br>
  <br>
  <h3>Hospitals:</h3>
  <div class="all-hospitals">
    {{#each Hospitals}}
      <div class="hospital-box" id="{{id}}">
        <h3>Hospital Name: {{hospital_name}}</h3>
        <h4>Address: {{street_address}}, {{city}} {{zip_code}}</h4>
        <button class="delete-hospital" id="delete-hospital-{{id}}">Remove Hospital</button>
      </div>
    {{/each}}
  </div>
  <br>
  <br>
  <h3>Shifts:</h3>
  {{#each Shifts}}
    <div class="shift-box">
      <h3>Shift Block: {{work_shift}} on {{day_of_week}}</h3>
      <h4>Duration: {{schedule_duration}} weeks</h4>
      <button class="delete-shift">Remove Shift</button>
    </div>
  {{/each}}
  <br>
  <button class="edit-profile" id="edit-profile">Edit Profile</button>
  <br>
  <button class="delete-profile"user-id={{id}}>Remove Profile</button>
  <script src="/js/profile.js"></script>
  <script src="/js/cloudinary.js"></script>
</div>